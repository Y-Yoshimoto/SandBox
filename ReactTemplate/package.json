{
  "name": "react-template",
  "description": "React portal template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:tsc": "npm run tsc && vite build",
    "preview": "npm run build && vite preview",
    "lint": "eslint . --max-warnings 0",
    "lint:report": "eslint . --max-warnings 0 --format html --output-file eslint-report.html",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "tsc": "tsc -b",
    "test": "vitest",
    "playwright": "playwright test",
    "playwright:ui": "playwright test --ui-host=0.0.0.0",
    "playwright:codegen": "playwright codegen",
    "playwright:report": "playwright show-report --port 9323",
    "coverage": "vitest run --coverage --silent",
    "update": "ncu -u && rm package-lock.json && npm install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "react-use": "^17.6.0",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "npm-check-updates": "^17.1.12",
    "playwright": "^1.49.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^2.1.8"
  },
  "browserslist": [
    "defaults"
  ]
}
